# Copy this file to .env and fill in your values.
# IMPORTANT: In Create React App, env vars are injected at build time and must be prefixed with REACT_APP_.

# Required for Supabase
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
REACT_APP_SUPABASE_KEY=YOUR-ANON-KEY

# Optional: set the site URL used in auth redirects (falls back to current origin or http://localhost:3000)
REACT_APP_SITE_URL=http://localhost:3000

# Optional: backend API base URL (if/when a backend is added)
REACT_APP_API_BASE_URL=

# Optional: used by the Venmo link in Dues page
REACT_APP_VENMO_HANDLE=
